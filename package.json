{
  "name": "ai-trading-bot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run validate:env:quick && next dev -p 3000",
    "build": "next build",
    "start": "npm run validate:env:quick && next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test:unit": "cross-env NODE_ENV=test jest --testPathPatterns=__tests__/unit",
    "test:integration": "cross-env NODE_ENV=test jest --testPathPatterns=__tests__/integration",
    "test:alpaca": "cross-env NODE_ENV=test jest --testPathPatterns=alpaca-api.test.ts",
    "test:environment-check": "cross-env NODE_ENV=test jest --testPathPatterns=environment-check.test.ts",
    "test": "cross-env NODE_ENV=test jest",
    "test:env": "cross-env NODE_ENV=test jest --testPathPatterns=enhanced-config-validator.test.ts",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test jest --detectOpenHandles --verbose --runInBand",
    "test:clean": "jest --clearCache",
    "test:win": "set NODE_ENV=test&& jest",
    "test:integration:win": "set NODE_ENV=test&& jest --testPathPatterns=__tests__/integration",
    "test:alpaca:win": "set NODE_ENV=test&& jest --testPathPatterns=alpaca-api.test.ts",
    "test:environment-check:win": "set NODE_ENV=test&& jest --testPathPatterns=environment-check.test.ts",
    "validate:env": "node scripts/startup-validation.js",
    "validate:env:quick": "node scripts/startup-validation.js --quick",
    "validate:env:template": "node scripts/startup-validation.js --template",
    "predev": "npm run validate:env:quick",
    "prestart": "npm run validate:env:quick",
    "health:check": "npm run validate:env && npm run supabase:test && npm run alpaca:test",
    "health:quick": "npm run validate:env:quick",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "analyze": "cross-env ANALYZE=true npm run build",
    "db:migrate": "node scripts/run-migrations.js",
    "db:seed": "node scripts/populate-demo-data.ts",
    "db:generate": "prisma generate",
    "supabase:test": "node scripts/test-supabase-connection.js",
    "supabase:setup": "node scripts/setup-database.js",
    "supabase:migrate": "supabase db push",
    "supabase:migration:new": "supabase migration new",
    "supabase:migration:apply": "node scripts/create-tables-api.js",
    "supabase:reset": "supabase db reset",
    "supabase:link": "supabase link",
    "alpaca:test": "node scripts/test-alpaca-connection.js",
    "setup:env": "npm run validate:env:template > .env.template && echo Template created: .env.template",
    "setup:complete": "npm run setup:env && npm run validate:env && npm run supabase:setup",
    "prod:check": "npm run validate:env && npm run test:coverage && npm run build",
    "prod:validate": "cross-env NODE_ENV=production npm run validate:env",
    "test:e2e": "playwright test",
    "test:ci": "jest --ci --coverage --maxWorkers=2"
  },
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^3.1.3",
    "@heroicons/react": "^2.2.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "framer-motion": "^12.23.22",
    "helmet": "^8.1.0",
    "immer": "^10.1.3",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.0.0",
    "ws": "^8.18.3",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^16.3.0",
    "@types/decimal.js": "^0.0.32",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  }
}
